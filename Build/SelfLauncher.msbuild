<Project InitialTarget="SetupSingleEXE">

    <!-- Yes I manually copy them instead the *.* because it wont work like it will work for few build and wont copy unless i did weird things -->

    <Target Name="SetupSingleEXE">
        <Exec Command="cd.. &amp;&amp; cd SelfLauncher &amp;&amp; msbuild /property:Configuration=$(Configuration) &amp;&amp; exit"/>
        <Copy SourceFiles="../SelfLauncher/bin/Release/Launcher%20DL.exe" DestinationFolder="./RELEASE/" Condition="$(Configuration) == Release" ContinueOnError="WarnAndContinue"/>
        <Copy SourceFiles="../SelfLauncher/bin/Debug/Launcher%20DL.exe" DestinationFolder="./DEBUG/" Condition="$(Configuration) == Debug" ContinueOnError="WarnAndContinue"/>
    </Target>
</Project>