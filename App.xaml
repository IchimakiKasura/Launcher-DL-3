<Application x:Class="Launcher_DL.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  			 Startup="App_Startup"
			 Exit="App_Exit"
             StartupUri="MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="ResourceDictionary/Resources.xaml"/>
				<ResourceDictionary Source="ResourceDictionary/ComboBoxResources.xaml"/>
				<ResourceDictionary Source="ResourceDictionary/ButtonResource.xaml"/>
				<ResourceDictionary Source="ResourceDictionary/RichTextBoxResource.xaml"/>
				<ResourceDictionary Source="ResourceDictionary/ScrollViewResource.xaml"/>
				<ResourceDictionary Source="ResourceDictionary/TextboxResource.xaml"/>
				<ResourceDictionary Source="ResourceDictionary/ProgressBarResource.xaml"/>
				<ResourceDictionary Source="ResourceDictionary/ContextMenuResource.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>