<Application x:Class="Launcher_DL_v6.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Launcher_DL_v6"
             StartupUri="MainWindow.xaml"
  			 Startup="App_Startup"
			 Exit="App_Exit">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Themes/Scrollview.xaml"/>
				<ResourceDictionary Source="Themes/Buttons.xaml"/>
				<ResourceDictionary Source="Themes/DropDown.xaml"/>
				<ResourceDictionary Source="Themes/RichTextBox.xaml"/>
				<ResourceDictionary Source="Themes/Textbox.xaml"/>
				<ResourceDictionary Source="Themes/Labels.xaml"/>
				<ResourceDictionary Source="Themes/ProgressBar.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
