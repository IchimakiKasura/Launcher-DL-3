<Application x:Class="Launcher_DL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  			 Startup="App_Startup"
			 Exit="App_Exit"
             StartupUri="MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Themes/Labels.xaml"/>
				<ResourceDictionary Source="Themes/Textbox.xaml"/>
				<ResourceDictionary Source="Themes/Buttons.xaml"/>
				<ResourceDictionary Source="Themes/DropDown.xaml"/>
				<ResourceDictionary Source="Themes/Scrollview.xaml"/>
				<ResourceDictionary Source="Themes/RichTextBox.xaml"/>
				<ResourceDictionary Source="Themes/ProgressBar.xaml"/>
				<ResourceDictionary Source="Themes/ContextMenu.xaml"/>
				<ResourceDictionary Source="Themes/ToolTip.xaml"/>
				<ResourceDictionary Source="Themes/LowBudget.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
